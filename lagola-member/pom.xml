    <groupId>com.billion</groupId>
    <artifactId>lagola-member</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>lagola-member</name>
    <description>拉勾拉会员模块</description>

    <properties>
        <java.version>1.8</java.version>
        <spring-cloud.version>Hoxton.RELEASE</spring-cloud.version>
    </properties>

    <dependencies>

        <dependency>
            <groupId>com.billion</groupId>
            <artifactId>lagola-common</artifactId>
